{
	"apps": [
		{
			"name": "name",
			"script": "./src/index.js",
			"cwd": ".",
			"interpreter": "node",
			"exec_interpreter": "node",
			"exec_mode": "cluster",
			"instances": 1,
			"increment_var": "PORT",
			"interpreter_args": "-r esm",
			"args": "-r esm",
			"max_memory_restart": "256M",
			"source_map_support": true,
			"instance_var": "NODE_APP_INSTANCE",
			"treekill": true,
			"min_uptime": 5000,
			"listen_timeout": 8000,
			"kill_timeout": 1600,
			"wait_ready": false,
			"max_restarts": 100000,
			"restart_delay": 4000,
			"autorestart": true,
			"cron_restart": "1 0 * * *",
			"vizion": false,
			"force": true,
			"shutdown_with_message": true,
			"log_date_format": "YYYY-MM-DD HH:mm Z",
			"error_file": "./logs/errors/error.log",
			"out_file": "./logs/out.log",
			"combine_logs": true,
			"log_type": "raw",
			"env": {
                "CORS_HEADER": ["*"]
			},
			"env_development": {
				"namespace": "development",
				"NODE_ENV": "development",
				"node_args": "environment=development",
				"watch": false,
				"DATABASE01_USER": "",
				"DATABASE01_PASS": "",
				"DATABASE01_DB_NAME": "",
				"DATABASE01_HOST": "",
				"DATABASE01_PORT": 0,
				"SERVICE_NAME": "",
				"SERVICE_MOCK": 0,
				"ENDPOINT_HEALTH": "/.well-known/apollo/server-health",
				"SCHEMA_CONFIG": "pattern-001.yaml",
				"GRAPHQL_PATH": "/",
				"API_URL": "",
				"PORT": 0,
				"AUTO_RESTART": 1,
				"GATEWAY_AUTH": 1,
				"GATEWAY_AUTH_TYPE": "local",
				"GATEWAY_CREDENTIALS_USER": "123",
				"GATEWAY_CREDENTIALS_PASS": "123",
				"GATEWAY_URL": "localhost",
				"CORS_HEADER": ["*"]
			},
			"env_localhostp": {
				"namespace": "localhostp",
				"NODE_ENV": "localhostp",
				"node_args": "environment=localhostp",
				"watch": [
					"src/schema/types",
					"src/index.js",
					"package.json"
				],
				"DATABASE01_USER": "",
				"DATABASE01_PASS": "",
				"DATABASE01_DB_NAME": "",
				"DATABASE01_HOST": "",
				"DATABASE01_PORT": 0,
				"SERVICE_NAME": "",
				"SERVICE_MOCK": 0,
				"ENDPOINT_HEALTH": "/.well-known/apollo/server-health",
				"SCHEMA_CONFIG": "pattern-001.yaml",
				"GRAPHQL_PATH": "/",
				"API_URL": "",
				"PORT": 0,
				"AUTO_RESTART": 1,
				"GATEWAY_AUTH": 1,
				"GATEWAY_AUTH_TYPE": "local",
				"GATEWAY_CREDENTIALS_USER": "123",
				"GATEWAY_CREDENTIALS_PASS": "123",
				"GATEWAY_URL": "localhost",
				"CORS_HEADER": ["*"]
			},
			"env_localhosta": {
				"namespace": "localhosta",
				"NODE_ENV": "localhosta",
				"node_args": "environment=localhosta",
				"watch": [
					"src/schema/types",
					"src/index.js",
					"package.json"
				],
				"DATABASE01_USER": "",
				"DATABASE01_PASS": "",
				"DATABASE01_DB_NAME": "",
				"DATABASE01_HOST": "",
				"DATABASE01_PORT": 0,
				"SERVICE_NAME": "",
				"SERVICE_MOCK": 0,
				"ENDPOINT_HEALTH": "/.well-known/apollo/server-health",
				"SCHEMA_CONFIG": "pattern-001.yaml",
				"GRAPHQL_PATH": "/",
				"API_URL": "",
				"PORT": 0,
				"AUTO_RESTART": 1,
				"GATEWAY_AUTH": 1,
				"GATEWAY_AUTH_TYPE": "local",
				"GATEWAY_CREDENTIALS_USER": "123",
